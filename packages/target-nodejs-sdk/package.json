{
  "name": "@adobe/target-nodejs-sdk",
  "version": "1.0.3",
  "description": "Adobe Target Node.js SDK, Delivery API client",
  "main": "lib/targetclient.server.js",
  "browser": "lib/targetclient.browser.js",
  "engines": {
    "node": ">=8.16.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && npm run format && npm run lint",
    "build": "rollup -c rollup.config.js",
    "www": "npm run build && node ./sample/server.js",
    "pretest": "npm run lint-test",
    "test": "jest",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "lint-src": "eslint src/**",
    "lint-test": "eslint test/**",
    "lint": "npm run lint-src && npm run lint-test",
    "format": "prettier --write {src,test}/**/*.js",
    "prepublishOnly": "echo //registry.npmjs.org/:_authToken=${NPM_TOKEN} > .npmrc"
  },
  "pre-commit": [
    "precommit-msg",
    "format",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/target-nodejs-sdk.git"
  },
  "keywords": [
    "NodeJS",
    "Server",
    "API",
    "Adobe",
    "Target",
    "MCID",
    "Visitor",
    "Delivery"
  ],
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@adobe/target-tools": "^1.0.0",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "ajv": "^5.5.2",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "mockdate": "^2.0.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.1",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-terser": "^5.1.1"
  },
  "dependencies": {
    "@adobe-mcid/visitor-js-server": "^2.0.0",
    "@adobe/target-decisioning-engine": "^1.0.0",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0",
    "whatwg-fetch": "^3.0.0"
  }
}
